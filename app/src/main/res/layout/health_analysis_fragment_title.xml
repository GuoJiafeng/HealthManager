<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:paddingTop="20dp"
    >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        >

        <TextView

            android:id="@+id/health_analysis_fragment_comprehensive_analysis_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="综合分析" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center">
        <TextView
            android:id="@+id/health_analysis_fragment_ecg_analysis_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="心电图分析"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center">
        <TextView
            android:id="@+id/health_analysis_fragment_report_analysis_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="报表分析"/>
    </LinearLayout>





</LinearLayout>